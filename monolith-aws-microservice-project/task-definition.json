{
  "family": "crypto-app",
  "containerDefinitions": [
    {
      "name": "crypto-app",
      "image": "748165670380.dkr.ecr.us-east-1.amazonaws.com/crypto-app:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group":  "/ecs/crypto-app",
          "awslogs-region": "eu-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "executionRoleArn": "arn:aws:iam::211125666158:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc"
}
